                                                                           <ul class="custom-menu">
                                                                               @if (Session["username"] == null)
                                                                               {
                                                                                   <li><a href="~/Userinfo/UserProfile"><i class="fa fa-user-o"></i> My Account</a></li>

                                                                               }
                                                                               else
                                                                               {
                                                                                   <li><a href="~/Userinfo/UserProfile"><i class="fa fa-user-o"></i> @Session["firstname"] </a></li>
                                                                               }
