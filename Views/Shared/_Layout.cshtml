                        <!-- Cart -->
                        <li class="header-cart dropdown default-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">




                                @if (Session["cart"] != null)
                                {
                                    int count = 1;
                                    int Total = 0;
                                    foreach (var item in (List<item>)Session["cart"])
                                    {
                                        int productPrice = Convert.ToInt32(item.ProductCartTable.Rows[0][6]);
                                        int lineTotal = Convert.ToInt32(item.Quantity * productPrice);
                                        Total = Convert.ToInt32(@Total + lineTotal);


                                    }
                                    <div class="header-btns-icon">
                                        <i class="fa fa-shopping-cart"></i>
                                        @*<span class="qty"> </span>*@
                                    </div>
                                    <strong class="text-uppercase">My Cart:</strong>
                                   <br />
                                    <span>
                                        @Total
                                    </span>

                                }




                            </a>
                            <div class="custom-menu">
                                <div id="shopping-cart">
                                    <div class="shopping-cart-list">
                                        @if (Session["cart"] != null)
                                        {
                                            int count = 1;
                                            int Total = 0;
                                            foreach (var item in (List<item>)Session["cart"])
                                            {
                                                <div class="product product-widget">
                                                    <div class="product-thumb">
                                                        <img src="../ProductImg/@item.ProductCartTable.Rows[0][8]" alt="">

                                                    </div>
                                                    <div class="product-body">
                                                        <h3 class="product-price">@item.ProductCartTable.Rows[0][6]  <span class="qty">x @item.Quantity</span></h3>
                                                        <h2 class="product-name"><a href="#">@item.ProductCartTable.Rows[0][3] </a></h2>
                                                    </div>
                                                    @*<button class="cancel-btn" onclick="location.href='@Url.Action("RemoveFromCart", "Home")?id='+@item.ProductCartTable.Rows[0][0])"><i class="fa fa-trash"></i></button>*@

                                                    <a class="cancel-btn" href="../Home/RemoveFromCart/@(item.ProductCartTable.Rows[0][0])"><i class="fa fa-trash"></i></a>
                                                </div>

                                            }
                                        }
                                        else {
                                            <p>No Product Added</p>
                                        }

                                    </div>
                                    <div class="shopping-cart-btns">
                                        @*  <button class="main-btn">View Cart</button>*@
                                        <button class="main-btn" onclick="location.href='@Url.Action("AddtoCartview", "Home")'">View Cart</button>

                                        <a class="btn primary-btn" href="~/Home/OrderDetails">Checkout <i class="fa fa-arrow-circle-right"></i></a>
                                    </div>
                                </div>
                            </div>
                        </li>
