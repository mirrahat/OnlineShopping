
@{
    ViewBag.Title = "MyProducts";
    Layout = "~/Views/Shared/_myadminlayout.cshtml";
}
@{ 

    
}
<h2>Productlist</h2>
<a href="../MyAdmin/Create" class="btn btn-info pull-right fa fa-plus">Add New Product</a>

<table class="table table-bordered table-striped">
    <tr>
        <th>Sr No</th>
        <th>Product Name </th>
        <th>Quanity</th>
        <th>SubcategoryName</th>
        <th>Action</th>
        <th>Action</th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        int count = 1;
    <tr>
        <td> @count</td>
        <td>@Model.Rows[i][3]</td>
        <td>@Model.Rows[i][5]</td>
        <td>@Model.Rows[i][2]</td>
        @*<td><a href="@Url.Action("Edit","MyAdmin", new { @ProductId =@Model.Rows[i][0]})">Edit</a></td>*@
        <td><a href="/MyAdmin/Edit/@(Model.Rows[i][0])">Edit</a></td>
        <td><a href="/MyAdmin/Delete/@(Model.Rows[i][0])">Delete</a></td>
    </tr>
        count++;
    }

</table>
